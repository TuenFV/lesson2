<h1>All Categories</h1>
<br/>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Action</th>
    </tr>
  </thead>

  <tbody>
    <% @categories.each do |category| %>
      <tr>
        <td><%= category.name %></td>
        <td><%= category.description %></td>
        <td>
          <%= link_to 'Show', admin_category_path(category) %>
        </td>
      </tr>
    <% end%>
  </tbody>
</table>
